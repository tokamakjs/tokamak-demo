{
  "presets": [["@babel/preset-react", { "runtime": "automatic" }], ["@babel/preset-env"]],
  "plugins": [
    ["@babel/plugin-transform-runtime"],
    [
      "@emotion/babel-plugin",
      {
        "sourceMap": true,
        "autoLabel": "always",
        "labelFormat": "[filename]__[local]"
      }
    ]
  ]
}
